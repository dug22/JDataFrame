    <modelVersion>4.0.0</modelVersion>

    <groupId>io.github.dug22</groupId>
    <artifactId>jdataframe</artifactId>
    <version>1.0.4</version>
    <packaging>jar</packaging>


    <properties>
        <maven.compiler.source>21</maven.compiler.source>
